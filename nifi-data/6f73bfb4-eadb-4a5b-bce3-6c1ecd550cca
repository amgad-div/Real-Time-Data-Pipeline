. Provocative, eye-opening, and often surprisingly funny, Made to Stick shows us the vital principles of winning ideas–and tells us how we can apply these rules to making our own messages stick. ...more","upc":"b96fc7a0ea7d4b45","price_excl_tax":"Dy0=","price_incl_tax":"Dy0=","category":"Business","available_quantity":14},"source":{"version":"2.6.1.Final","connector":"postgresql","name":"books_db","ts_ms":1751578755996,"snapshot":"true","db":"web_scraper","sequence":"[null,\"2513305520\"]","ts_us":1751578755996565,"ts_ns":1751578755996565000,"schema":"public","table":"books","txId":940,"lsn":2513305520,"xmin":null},"op":"r","ts_ms":1751578756303,"ts_us":1751578756303495,"ts_ns":1751578756303495000,"transaction":null}},{"schema":{"type":"struct","fields":[{"type":"struct","fields":[{"type":"int32","optional":false,"default":0,"field":"id","name":null,"version":null,"parameters":null},{"type":"string","optional":false,"default":null,"field":"title","name":null,"version":null,"parameters":null},{"type":"bytes","optional":true,"default":null,"field":"price","name":"org.apache.kafka.connect.data.Decimal","version":1,"parameters":{"scale":"2","connect.decimal.precision":"6"}},{"type":"string","optional":true,"default":null,"field":"rating","name":null,"version":null,"parameters":null},{"type":"string","optional":true,"default":null,"field":"image_url","name":null,"version":null,"parameters":null},{"type":"string","optional":true,"default":null,"field":"detail_url","name":null,"version":null,"parameters":null},{"type":"string","optional":true,"default":null,"field":"description","name":null,"version":null,"parameters":null},{"type":"string","optional":true,"default":null,"field":"upc","name":null,"version":null,"parameters":null},{"type":"bytes","optional":true,"default":null,"field":"price_excl_tax","name":"org.apache.kafka.connect.data.Decimal","version":1,"parameters":{"scale":"2","connect.decimal.precision":"6"}},{"type":"bytes","optional":true,"default":null,"field":"